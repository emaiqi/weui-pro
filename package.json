{
  "name": "weui-pro",
  "version": "1.1.1",
  "description": "一款基于 Vue.js 2.0 的前端 UI 组件库，开发和服务于微信小程序。",
  "homepage": "https://weui-pro.github.io",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "dev": "nodemon build/dev-server",
    "lint": "eslint --ext .js,.vue src docs build --ignore-pattern test/unit/specs test/e2e/specs",
    "dist": "npm run build:component && npm run build:docs",
    "build:docs": "webpack --config build/webpack.prod.conf.js -p",
    "build:component": "node build/component",
    "publish:docs": "npm run build:docs && gh-pages -d dist -b master -r https://github.com/weui-pro/weui-pro.github.io.git -m '更新站点'",
    "publish:component": "node build/component"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:weui-pro/weui-pro.git"
  },
  "bugs": {
    "url": "https://github.com/WeUI-Pro/weui-pro/issues"
  },
  "keywords": [
    "ui",
    "weui",
    "framework",
    "component"
  ],
  "files": [
    "src"
  ],
  "author": "Vultur <wall-e@Live.cn>",
  "license": "MIT",
  "dependencies": {
    "at-ui": "^1.3.3",
    "at-ui-style": "^1.5.1",
    "vue": "^2.5.13",
    "vue-router": "^2.8.1",
    "vue-template-compiler": "^2.5.13"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cheerio": "^0.22.0",
    "clipboard": "^1.7.1",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.2.0",
    "highlight.js": "^9.12.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "json-loader": "^0.5.7",
    "markdown-it": "^8.4.0",
    "markdown-it-container": "^2.0.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "ora": "^1.3.0",
    "postcss-loader": "^2.0.10",
    "postcss-url": "^7.3.0",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.8",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^12.2.2",
